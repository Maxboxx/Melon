

-- A kiwi variable
enum Var
	-- A named variable
	name(string),
	
	-- A derefed variable
	deref(string)
	
	--/
	-- A sub variable
	sub {
		Var*: v,
		string: p
	}
	
	-- A typed variable
	type {
		Var*: v,
		string: t
	}
	/--
	
	fun string: toKiwi()
		ret match self do
			string: n as Var.name:  n
			string: n as Var.deref: "*" + n
			
			else: ""
		end
	end
end
