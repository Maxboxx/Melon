
struct Vector
	byte: x, y, z
	
	function init()
		self.x, self.y, self.z = 0, 0, 0
	end
	
	function init(byte: b)
		self.x, self.y, self.z = b, b, b
	end
	
	function init(byte: x, byte: y, byte: z)
		self.x, self.y, self.z = x, y, z
	end
	
	static function operator Vector: +(Vector: v1, Vector: v2)
		return Vector {
			x = v1.x + v2.x,
			y = v1.y + v2.y,
			z = v1.z + v2.z
		}
	end
	
	function operator Vector: -()
		return Vector {
			x = -self.x,
			y = -self.y,
			z = -self.z
		}
	end
	
	static function operator bool: ==(Vector: v1, Vector: v2)
		return if v1.x ~= v2.x then
			false
		elseif v1.y ~= v2.y then
			if v1.x ~= v2.x then
				false
			elseif v1.y ~= v2.y then
				false
			else
				v1.z == v2.z
			end
		else
			v1.z == v2.z
		end
	end
end

