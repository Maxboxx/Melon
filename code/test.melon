
struct Value<T>
	T: value
	
	function init(T: value)
		self.value = value
	end
	
	function T: get()
		T: t = 1
		return self.value
	end
	
	function set(T: value)
		self.value = value
	end
end

int: x, y = 45, 56

Value<int>: value = Value<int>(x)

int: a = value:get()

value:set(y)

int: b = value:get()
