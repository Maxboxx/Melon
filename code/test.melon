
struct Comparison
	static function T: min<T>(T: a, T: b)
		return if a < b -> a else b
	end
	
	static function T: max<T>(T: a, T: b)
		return if a > b -> a else b
	end
	
	static function T: clamp<T>(T: value, T: min, T: max)
		return if value < min then
			min
		elseif value > max then
			max
		else
			value
		end
	end
end


int: a = Comparison.min<int>(3, 4)
int: b = Comparison.max<int>(3, 4)
int: c = Comparison.clamp<int>(4, 3, 5)
