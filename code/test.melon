
include Melon.Console.log

enum E
	none,
	number(int),
	boolean(bool),
	character(char)
	
	fun string: type()
		ret if self is E.none do
			"none"
		elif self is E.number do
			"number"
		elif self is E.boolean do
			"boolean"
		elif self is E.character do
			"character"
		else
			"?"
		end
	end
end

E[#]: values = {E.none, E.number(3), E.boolean(true), E.character('A')}

for int: i = 0, < #values do
	log(values[i].type())
end
