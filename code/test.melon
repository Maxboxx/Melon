
struct Value<T>
	T: value
	
	function init(T: value)
		self.value = value
	end
	
	function T: get()
		return self.value
	end
	
	function set(T: value)
		self.value = value
	end
end

int: x, y = 45, 56

Value<Value<int>>: value = Value<Value<int>>(Value<int>(x))

int: a = value.value:get()

value.value:set(y)

int: b = value:get():get()
